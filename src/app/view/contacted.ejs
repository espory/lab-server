<!-- breadcrumb start-->
<section class="breadcrumb breadcrumb_bg">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <div class="breadcrumb_iner text-center">
                    <div class="breadcrumb_iner_item">
                        <h2>Join us</h2>
                        <p>Home<span>-</span>Join us</p>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
<!-- breadcrumb start-->

<!-- ================ contact section start ================= -->
<section class="contact-section section_padding">
    <div class="container">
        <div class="row home_banner">
            <div class="col-12">
                <h2 class="contact-title">招生求贤 - Recruitment</h2>
            </div>
            <% for(let i = 0;i < recruit.length;i++){ %>
                <div class="col-lg-12 recruit">
                    <% if(recruit[i].title != null && recruit[i].title != ''){ %>
                        <h4><%= recruit[i].title %></h4>
                    <% } %>
                    <% if(recruit[i].contents != null && recruit[i].contents != ''){ %>
                        <p><%= recruit[i].contents %></p>
                    <% } %>
                </div>
            <% } %>
        </div>
    </div>
</section>
<!-- ================ contact section end ================= -->
